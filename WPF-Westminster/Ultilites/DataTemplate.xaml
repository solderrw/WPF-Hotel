<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WPF_Westminster.ViewsModel"
                    xmlns:view="clr-namespace:WPF_Westminster.Views">


    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomerVM}">
        <view:Customers/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <view:Products/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:userVM}">
        <view:user/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UserZaprocVM}">
        <view:UserZaproc/>
    </DataTemplate>


</ResourceDictionary>